{
  "productName": "gongyi",
  "version": "0.1.0",
  "identifier": "com.gongyi.desktop",
  "build": {
    "beforeDevCommand": "npm run dev --prefix ../web",
    "beforeBuildCommand": "npm run build --prefix ../web",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../../web/dist"
  },
  "app": {
    "windows": [
      {
        "title": "铜陵杨铜工艺计算器",
        "width": 1000,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
